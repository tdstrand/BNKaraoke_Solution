  "private": true,
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@csstools/normalize.css": "^12.1.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@microsoft/signalr": "^8.0.0",
    "@types/axios": "^0.9.36",
    "axios": "^1.8.4",
    "fuse.js": "^7.1.0",
    "lodash": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "file:react-hot-toast-2.4.1.tgz",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-router-dom": "^7.5.0",
    "react-scripts": "5.0.1",
    "sanitize.css": "^13.0.0",
    "web-vitals": "^4.2.4",
    "workbox-core": "^6.6.0",
    "workbox-precaching": "^6.6.0",
    "workbox-routing": "^6.6.0"
  },
  "scripts": {
    "start": "cross-env HOST=0.0.0.0 react-scripts start",
    "build": "react-scripts build && workbox injectManifest workbox-config.js",
    "build:dev": "cross-env NODE_ENV=development react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "warn",
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "off"
